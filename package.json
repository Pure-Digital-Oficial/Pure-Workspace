{
  "name": "@pure-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many -t serve",
    "start:front-summons": "nx run web-summons:serve",
    "start:front-pure-tv": "nx run web-pure-tv:serve",
    "start:front-pure-marketing": "nx run web-pure-marketing:serve",
    "start:back-summons": "nx run api-summons:serve",
    "start:back-pure-tv": "nx run api-pure-tv:serve",
    "start:back-general": "nx run api-general:serve",
    "start:back-pure-finance": "nx run api-pure-finance:serve",
    "start:back-pure-marketing": "nx run api-pure-marketing:serve",
    "start:back-pure-blog": "nx run api-pure-blog:serve",
    "start:pure-marketing": "nx run-many --target=serve --projects=web-pure-marketing,api-pure-marketing --parallel",
    "start:pure-tv": "nx run-many --target=serve --projects=web-pure-tv,api-general,api-pure-tv --parallel",
    "start:pure-the-creator": "nx run-many --target=serve --projects=web-pure-the-creator,api-general,api-pure-finance --parallel",
    "start:summons": "nx run-many --target=serve --projects=web-summons,api-general,api-summons --parallel",
    "build:back-general": "nx build api-general",
    "build:back-pure-tv": "nx build api-pure-tv",
    "build:back-pure-marketing": "nx build api-pure-marketing",
    "build:front-pure-marketing": "nx build web-pure-marketing",
    "build:front-pure-tv": "nx build web-pure-tv",
    "lint": "nx format:check --all",
    "lint:write": "nx format:write --all",
    "test": "nx run-many -t test",
    "test:coverage": "nx run-many -t test --codeCoverage",
    "update-lock:dev:marketing": "node configs/scripts/update-files/update-lock.js postgresql ../../../prisma/marketing/migrations",
    "update-lock:prod:marketing": "node configs/scripts/update-files/update-lock.js cockroachdb ../../../prisma/marketing/migrations",
    "update-lock:dev:general": "node configs/scripts/update-files/update-lock.js postgresql ../../../prisma/general/migrations",
    "update-lock:prod:general": "node configs/scripts/update-files/update-lock.js cockroachdb ../../../prisma/general/migrations",
    "prisma:generate": "npm run prisma:generate:general && npm run prisma:generate:marketing",
    "prisma:generate-prod": "npm run prisma:generate-prod:general && npm run prisma:generate-prod:marketing",
    "prisma:generate:general": "npx prisma generate --schema=./prisma/general/schema.prisma",
    "prisma:generate:marketing": "npx prisma generate --schema=./prisma/marketing/schema.prisma",
    "prisma:generate-prod:marketing": "npx prisma generate --schema=./prisma/marketing/schema-production.prisma",
    "prisma:generate-prod:general": "npx prisma generate --schema=./prisma/general/schema-production.prisma",
    "prisma:migrate": "npm run prisma:migrate:general && npm run prisma:migrate:marketing",
    "prisma:migrate-prod": "npm run prisma:migrate-prod:general && npm run prisma:migrate-prod:marketing",
    "prisma:migrate:general": "npm run update-lock:dev:general && npx prisma migrate dev --schema=./prisma/general/schema.prisma",
    "prisma:migrate:marketing": "npm run update-lock:dev:marketing && npx prisma migrate dev --schema=./prisma/marketing/schema.prisma",
    "prisma:migrate-prod:marketing": "npm run update-lock:prod:marketing && npx prisma db push --schema=./prisma/marketing/schema-production.prisma",
    "prisma:migrate-prod:general": "npm run update-lock:prod:general && npx prisma db push --schema=./prisma/general/schema-production.prisma",
    "prisma:studio": "prisma studio",
    "docker": "docker-compose up",
    "docker:postgres": "docker-compose -f docker-compose-postgres.yaml up",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
